{
    "name": "mewesk/twig-spreadsheet-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle integrates PhpSpreadsheet into Symfony using Twig.",
    "keywords": ["excel","phpexcel","spreadsheet","phpspreadsheet","twig","symfony"],
    "homepage": "https://github.com/MewesK/TwigSpreadsheetBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mewes Kochheim",
            "email": "mewes@kochheim.de"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/MewesK/TwigSpreadsheetBundle/graphs/contributors"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~3.2|~4.0",
        "twig/twig": "~2.0",
        "phpoffice/phpspreadsheet": "dev-develop"
    },
    "require-dev": {
        "symfony/symfony": "~3.2|~4.0",
        "symfony/phpunit-bridge": "~3.2|~4.0",
        "sensio/framework-extra-bundle": "~3.2|~4.0",
        "mpdf/mpdf": "~7.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/MewesK/PhpSpreadsheet"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "mpdf/mpdf": "Add support for PDF rendering, requires ~7.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigSpreadsheetBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "MewesK\\TwigSpreadsheetBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
