{% macro cell(a) %}
    {% xlscell %}{{ a }}{% endxlscell %}
{% endmacro %}

{% macro content(a) %}
    {{ a }}
{% endmacro %}

{% macro row(a, b) %}
    {% xlsrow %}
        {% xlscell %}{{ a }}{% endxlscell %}
        {% xlscell %}{{ b }}{% endxlscell %}
    {% endxlsrow %}
{% endmacro %}

{% macro sheet(a, b, c) %}
    {% xlssheet a %}
        {% xlsrow %}
            {% xlscell %}{{ b }}{% endxlscell %}
            {% xlscell %}{{ c }}{% endxlscell %}
        {% endxlsrow %}
    {% endxlssheet %}
{% endmacro %}

{% macro sheet2(a, b, c) %}
    {% from _self import cell %}
    {% xlssheet a %}
        {% xlsrow %}
            {{ cell(b) }}
            {{ cell(c) }}
        {% endxlsrow %}
    {% endxlssheet %}
{% endmacro %}
